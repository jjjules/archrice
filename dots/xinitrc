xrdb -merge $HOME/.Xresources

$SCRIPTS_PATH/setkb
$SCRIPTS_PATH/setwp
unclutter &
dunst &
redshift -l 46.5:6.64 -g 0.8 &
$SCRIPTS_PATH/mute on &
$SCRIPTS_PATH/lockhook &

# Default to single monitor when on desktop (Not working but keep for command)
if [ "$(sudo dmidecode -t baseboard | grep -i manufacturer | awk '{print tolower($2)}')" = 'asustek' ]; then
	$SCRIPTS_PATH/autolayout d1
fi

exec i3
