{
    "files.autoSave": "afterDelay",
    "terminal.integrated.inheritEnv": false,
    "keyboard.dispatch": "keyCode",
    "editor.lineNumbers": "relative",
    "vim.smartRelativeLine": true,
    "zenMode.hideLineNumbers": false,
    "python.analysis.autoImportCompletions": false,
    "zenMode.hideStatusBar": false,
    "window.menuBarVisibility": "toggle",
    "notebook.output.textLineLimit": 1000,
    "python.condaPath": "/usr/bin/micromamba",
    "python.languageServer": "Pylance",
    // python.linting settings are deprecated
    // https://code.visualstudio.com/docs/python/linting
    // "python.linting.enabled": true,
    // "python.linting.lintOnSave": true,
    // "python.linting.flake8Enabled": true,
    // "python.linting.maxNumberOfProblems": 250,
    // "python.formatting.provider": "black",
    // "python.formatting.blackArgs": [
    //     "--line-length",
    //     "130"
    // ],
    "python.linting.flake8Args": [
        "--max-line-length=130",
        "--ignore=E402,E302,E305,E122,E126,E127,E128,E203,E225,E226,E231,E261,E262,E266,E303,E501",
    ],
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false
    },
    "vim.easymotion": true,
    "vim.foldfix": true,
    "notebook.lineNumbers": "on",
    "git.mergeEditor": false,
    "explorer.confirmDragAndDrop": false,
    "vim.normalModeKeyBindings": [
        {
            "before": ["<C-n>"],
            "commands": [":set hlsearch!"]
        },
        {
            "before": ["z", "s"],
            "after": [
                "0", "f", ",", "l", "s", "<ENTER>", "<ESC>",
            ],
        },
        {
            "before": ["z", "e"],
            "after": [
                "0", "f", ")", "i", "<ENTER>", "<BACKSPACE>", "<ESC>",
            ],
        },
        {
            "before": ["Y"],
            "after": ["y", "$"],
        },
    ],
    "python.linting.flake8Path": "/usr/lib/python3.10/site-packages/flake8/__main__.py",
    "vim.vimrc.path": "$HOME/.config/VSCodium/User/vimrc",
    "vim.vimrc.enable": true,
    "jupyter.widgetScriptSources": [
        "jsdelivr.com",
        "unpkg.com"
    ],
    "gitlens.hovers.currentLine.over": "line",
    "outline.showVariables": false,
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "git.confirmSync": false,
    "editor.inlineSuggest.enabled": true,
    "workbench.colorTheme": "Default Dark+ Experimental",
    "jupyter.logging.level": "verbose",
    "jupyter.kernels.trusted": [
        "/home/jules/.local/share/micromamba/envs/ariblue-backend/share/jupyter/kernels/python3/kernel.json"
    ],
    "github.copilot.enable": {
        "*": true,
        "yaml": false,
        "plaintext": true,
        "markdown": true
    },
    "gitlens.plusFeatures.enabled": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "jupyter.enableExtendedPythonKernelCompletions": true,
    "jupyter.completionTriggerCharacters": {
        "bash": [
            "$",
            "{"
        ],
        "java": [
            ".",
            "@",
            "#"
        ],
        "javascript": [
            ".",
            "\"",
            "'",
            "/",
            "@"
        ],
        "python": [
            ".",
            "%"
        ],
        "julia": [
            "."
        ],
        "r": [
            "[",
            "(",
            ",",
            "$",
            "@",
            "\"",
            "'"
        ],
        "rust": [
            ".",
            ":"
        ],
        "scala": [
            "."
        ],
        "typescript": [
            ".",
            "\"",
            "'",
            "/",
            "@",
            "<"
        ]
    },
    "python.createEnvironment.trigger": "off"
}
